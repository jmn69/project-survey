{
  "name": "jm-survey",
  "version": "1.0.0",
  "description": "survey app for projets",
  "main": "server/index.js",
  "scripts": {
    "build": "webpack -d --config config/webpack.dev.js --color",
    "watch": "webpack -d --config config/webpack.dev.js --watch --color",
    "prod": "webpack -p --config config/webpack.prod.js --color",
    "start": "npm run clean && cross-env NODE_ENV=development babel-watch server/index.js",
    "clean": "rimraf buildClient buildServer",
    "test": "jest && codecov",
    "heroku-postbuild": "webpack -p --config config/webpack.prod.js --progress"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/client/*.{js,jsx}",
      "!**/node_modules/**",
      "**/server/*.{js}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "author": "jmi69",
  "license": "ISC",
  "devDependencies": {
    "autodll-webpack-plugin": "^0.2.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-universal-import": "^1.2.5",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-watch": "^2.0.7",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "codecov": "^2.2.0",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.0.4",
    "css-loader": "^0.28.4",
    "enzyme": "^2.8.2",
    "expect": "^1.20.2",
    "express-session": "^1.15.3",
    "extract-css-chunks-webpack-plugin": "^2.0.16",
    "fetch-everywhere": "^1.0.5",
    "file-loader": "^0.11.2",
    "flexboxgrid": "^6.3.1",
    "history": "^4.6.3",
    "image-webpack-loader": "^3.3.1",
    "jest": "^20.0.4",
    "jquery": "^3.2.1",
    "moment": "^2.18.1",
    "mongoose": "^4.10.8",
    "morgan": "^1.8.2",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react-bootstrap": "^0.31.2",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.5",
    "react-responsive": "^1.3.4",
    "react-test-renderer": "^15.6.1",
    "react-universal-component": "^2.3.2",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-first-router": "^1.9.12",
    "redux-first-router-link": "^1.1.3",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "stats-webpack-plugin": "^0.6.1",
    "transition-group": "0.0.1",
    "url-loader": "^0.5.9",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-flush-chunks": "^1.1.18",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-hot-server-middleware": "^0.1.0",
    "write-file-webpack-plugin": "^4.1.0"
  },
  "dependencies": {
    "express": "^4.15.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "webpack": "^3.5.1"
  }
}
