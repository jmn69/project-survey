{
  "name": "jm-survey",
  "version": "1.0.0",
  "description": "survey app for projets",
  "main": "main.jsx",
  "scripts": {
    "build": "webpack -d --config config/webpack.dev.js --color",
    "watch": "webpack -d --config config/webpack.dev.js --watch --color",
    "prod": "webpack -p --config config/webpack.prod.js --color",
    "start": "node server.js",
    "test": "jest && codecov",
    "heroku-postbuild": "webpack -p --config config/webpack.prod.js --progress"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/client/*.{js,jsx}",
      "!**/node_modules/**",
      "**/server/*.{js}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "author": "jmi69",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "codecov": "^2.2.0",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.28.4",
    "enzyme": "^2.8.2",
    "expect": "^1.20.2",
    "express-session": "^1.15.3",
    "file-loader": "^0.11.2",
    "flexboxgrid": "^6.3.1",
    "image-webpack-loader": "^3.3.1",
    "jest": "^20.0.4",
    "material-ui": "^1.0.0-beta.2",
    "material-ui-icons": "^1.0.0-alpha.19",
    "moment": "^2.18.1",
    "mongoose": "^4.10.8",
    "morgan": "^1.8.2",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.5",
    "react-responsive": "^1.3.4",
    "react-router-dom": "^4.1.2",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.2"
  },
  "dependencies": {
    "express": "^4.15.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "webpack": "^3.3.0"
  }
}
